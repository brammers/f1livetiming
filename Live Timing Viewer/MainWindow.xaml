<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:uc="clr-namespace:Live_Timing_Viewer"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Class="Live_Timing_Viewer.MainWindow"
	x:Name="Main"
	Title="Live Timing"
	Width="640" Height="480"
	Background="Black" 
	mc:Ignorable="d" 
	ResizeMode="NoResize">
	
	<Grid x:Name="LayoutRoot">
		<uc:TimingGrid x:Name="timingGrid" Margin="17,126.5,113,19.5" Grid.Column="1" Grid.ColumnSpan="2"/>
		<TextBlock x:Name="title" Margin="17,8,113,0" VerticalAlignment="Top" Height="38" TextWrapping="Wrap" Foreground="White" FontSize="21.333" FontStyle="Italic" Grid.Column="1" Grid.ColumnSpan="2" />
	</Grid>
</Window>